<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFF"
    android:orientation="vertical"
    tools:context="com.share.bag.ui.activitys.mine.BusinessActivity">



    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/x144"
        android:id="@+id/relativeLayout2">

        <ImageView
            android:id="@+id/business_return"
            android:layout_width="20dp"
            android:layout_height="18dp"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:src="@mipmap/xiaoyuhei" />


        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="商务合作"
            android:textSize="@dimen/x46"
            android:textColor="@color/navigation_bar_text_color"/>


    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x4"
        android:background="@color/navigation_bar_line_color"/>


        <WebView
             android:id="@+id/business_mv"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"></WebView>



</LinearLayout>
